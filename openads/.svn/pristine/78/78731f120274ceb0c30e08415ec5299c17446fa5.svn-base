.tooltip {
  text-decoration:none;
  position:absolute;
}
 
.tooltip span.tooltip-span {
  display:none;
  -moz-border-radius:6px;
  -webkit-border-radius:6px;
  border-radius:6px;
  color:black;
  background:white; 
}
 
.tooltip span.tooltip-span img {
  float:left;
  margin:0px 8px 8px 0;
}
 
.tooltip:hover span.tooltip-span {
  display:block;
  position:absolute;
  top:0;
  left:0;
  z-index:1000;
  width:auto;
  max-width:128px;
  border:1px solid #eee;
  margin-top:12px;
  margin-left:32px;
  overflow:hidden;
  padding:8px;
}

