table.gridjs-table .task_state.task_error {
  background-color: #fcc;
  color: #b00;
}
table.tab-tab .task_state.task_draft,
table.gridjs-table .task_state.task_draft {
  background-color:  #E0CFFC ;
  color:  #6610f2;
}
