.widget-suivi-conso-stats {
  display: flex;
  justify-content: space-between;
  padding-bottom: 25px;
}
.widget-suivi-conso-stats .statistic h4 {
  font-size:12px;
  font-weight: 500;
  color: #8a8484;
  text-transform:uppercase;
  margin-left: 35px;
}
.widget-suivi-conso-stats .statistic .count {
  font-size:1.5em;
  font-weight:700;
}

.widget-suivi-conso-stats .statistic .disk {
  display:inline-block;
  height: 15px;
  width: 15px;
  border-radius:10px;
  margin-right: 20px;
}

.widget-suivi-conso-stats .statistic.info .disk,
.widget-suivi-conso-progress .signature-progress-bar.info {
  background-color: #36a2eb;
}
.widget-suivi-conso-stats .statistic.remaining .disk {
  background-color: #ccc;
}
.widget-suivi-conso-stats .statistic.warning .disk,
.widget-suivi-conso-progress .signature-progress-bar.warning {
  background-color: #ff7c00;
}
.widget-suivi-conso-stats .statistic.error .disk,
.widget-suivi-conso-progress .signature-progress-bar.error {
  background-color: #ce0505;
}

.widget-suivi-conso-progress {
  position: relative;
  font-size:2em;
  color: #fff;
  font-weight: 900;
  background: #ccc;
  width:100%;
  height: 44px;
  border-radius: 50px;
}

.widget-suivi-conso-progress .signature-progress-bar {
  border-radius: 50px;
  height:100%;
  min-width: 44px;
  max-width: 100%;
}

.widget-suivi-conso-progress .percent {
  position:absolute;
  top:0;
  left:0;
  height: 100%;
  width: 100%;
  margin:0;
  text-align:center;
  line-height: 44px;
}

.widget-content .container .statistic.error+.statistic.remaining .quota {
    display: none;
}

